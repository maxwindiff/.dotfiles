[alias]
    st = status
    ci = commit
    co = checkout
    di = diff
    dif = diff

    b = branch -vv
    bd = branch -d
    bD = branch -D
    bu = branch -u

    man = help
    head = log -1
    staged = diff --cached
    unstage = reset HEAD
    gg = log --abbrev-commit --relative-date --pretty='format: %C(green)%h %C(yellow)%d %C(reset)%s %C(green)%an %C(blue)(%ar)%n' --graph
    logg = log --name-status --graph
    stat = log --stat
[core]
	excludesfile = /Users/kichi/.gitignore_global
[push]
	default = current
[fetch]
	prune = true
[init]
	defaultBranch = main
